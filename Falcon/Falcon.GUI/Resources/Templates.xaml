<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:av="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:viewModel="clr-namespace:Falcon.GUI.ViewModel"
                    xmlns:view="clr-namespace:Falcon.GUI.View">
    <av:DataTemplate DataType="{x:Type viewModel:FalconViewModel}">
        <view:FalconView/>
    </av:DataTemplate>
    <av:DataTemplate DataType="{x:Type viewModel:ImportLoadoutsViewModel}">
        <view:ImportLoadoutsView/>
    </av:DataTemplate>
    <av:DataTemplate DataType="{x:Type viewModel:SelectLoadoutsViewModel}">
        <view:SelectLoadoutsView/>
    </av:DataTemplate>
    <av:DataTemplate DataType="{x:Type viewModel:MissionLoadoutEditorViewModel}">
        <view:MissionLoadoutEditorView/>
    </av:DataTemplate>
</ResourceDictionary>